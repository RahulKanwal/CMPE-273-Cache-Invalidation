{
  "version": 2,
  "name": "eds-marketplace",
  "builds": [
    {
      "src": "marketplace-ui/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "buildCommand": "cd marketplace-ui && npm install && npm run build",
  "outputDirectory": "marketplace-ui/build",
  "framework": "create-react-app"
}